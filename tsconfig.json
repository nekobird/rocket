{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "lib": [
      "dom.iterable",
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "outDir": "build/",
    "paths": {
      "~/*": [
        "rocket/*"
      ]
    },
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
  },
  "include": [
    "rocket/*",
  ],
  "exclude": [
    "node_modules",
    "source"
  ]
}