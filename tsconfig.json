{
  "compilerOptions": {
    "declaration": true,
    "module": "ES6",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "outDir": "build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": false,
    "strict": true,
    "target": "ES2015"
  },
  "include": [
    "rocket/*"
  ],
  "exclude": [
    "node_modules",
    "source/**"
  ]
}
