{
  "name": "@nekobird/rocket",
  "version": "0.0.130",
  "description": " A foundational library for creating interactive web experiences.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/nekobird/rocket.git"
  },
  "license": "MIT",
  "author": "Andrew Prasetya",
  "files": [
    "build/**/*"
  ],
  "main": "build/rocket.js",
  "types": "build/rocket.d.ts",
  "scripts": {
    "build": "rm -r build; tsc -t es6 --emitDeclarationOnly; npx parcel build ./rocket/rocket.ts --out-dir build",
    "build-playground": "npx parcel build ./playground/index.html --public-url ./",
    "build-ts": "rm -r build; tsc -t es6",
    "clean": "rm -r dist; rm -r build; rm -r .cache",
    "prepare": "npm run build",
    "start": "npx parcel ./playground/index.html",
    "test": "npx jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@nekobird/controls": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.18",
    "autoprefixer": "^9.6.4",
    "cssnano": "^4.1.10",
    "sass": "^1.23.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  }
}
